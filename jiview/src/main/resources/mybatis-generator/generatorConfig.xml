<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!-- 数据库驱动:选择你的本地硬盘上面的数据库驱动包-->
    <classPathEntry location="D:\workspace\work\tool\mybatis\通用Mapper\通用Mapper\lib\mysql-connector-java-5.1.18.jar"/>
    <context id="DB2Tables" targetRuntime="MyBatis3">
        <!-- 生成java文件的编码 -->
        <property name="javaFileEncoding" value="UTF-8"/>
        <!-- pojo里生成toString方法的插件 -->
        <plugin type="org.mybatis.generator.plugins.ToStringPlugin" />
        <!-- 生成通用Mapper的插件 -->
        <plugin type="tk.mybatis.mapper.generator.MapperPlugin">
            <!-- 生成的Mapper继承Mapper和MySqlMapper两个接口 -->
            <property name="mappers" value="tk.mybatis.mapper.common.Mapper,tk.mybatis.mapper.common.MySqlMapper"/>
            <!-- caseSensitive默认false，当数据库表名区分大小写时，可以将该属性设置为true -->
            <property name="caseSensitive" value="true"/>
        </plugin>

        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!--数据库链接URL，用户名、密码 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://119.57.73.147:3306/jiview_db" userId="evaluation"
                        password="lwq@mysql_Eval_123456">
        </jdbcConnection>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <!-- 生成模型的包名和位置-->
        <javaModelGenerator targetPackage="com.smaller.jiview.platform.system.model" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!-- 生成映射文件的包名和位置-->
        <sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources/mapper/system">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <!-- 生成DAO的包名和位置-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.smaller.jiview.platform.system.mapper" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>


        <!-- 要生成的表 tableName是数据库中的表名或视图名 domainObjectName是实体类名-->
        <table tableName="sys_user" domainObjectName="User" enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
            <property value="false" name="useActualColumnNames"/>
            <!-- 指定主键列，insert后可取到id值，identity表示是自增列-->
            <generatedKey column="user_id" sqlStatement="MySql" identity="true"/>
        </table>


        <table tableName="sys_menu" domainObjectName="Menu" enableCountByExample="false" enableUpdateByExample="false"
